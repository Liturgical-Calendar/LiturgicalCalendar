{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
            "calendar_id": {
                "$ref": "https://litcal.johnromanodorazio.com/api/dev/schemas/CommonDef.json#/definitions/DiocesanCalendarId"
            },
            "path": {
                "type": "string",
                "pattern": "^jsondata\\\/sourcedata\\\/nations\\\/[A-Z]{2}\\\/[a-zA-Z0-9 ()'-]+\\.json$"
            },
            "nation": {
                "$ref": "https://litcal.johnromanodorazio.com/api/dev/schemas/CommonDef.json#/definitions/Region"
            },
            "diocese": {
                "type": "string",
                "pattern": "^[a-zA-Z0-9 ()'-]+$"
            },
            "group": {
                "type": "string"
            }
        },
        "required": [
            "calendar_id",
            "path",
            "nation",
            "diocese"
        ]
    }
}
